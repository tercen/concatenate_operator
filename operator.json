{
  "name": "Concatenate",
  "description": "Concatenates strings contained in different variables.",
  "tags": ["text processing", "data transformation"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/concatenate_operator"],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers (column names)",
            "ontologyMapping": "variable",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Concatenated",
                "type": "string",
                "description": "Concatenated strings per row",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}